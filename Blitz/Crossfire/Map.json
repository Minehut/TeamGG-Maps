{
   "@proto": "1.3.6",
   "name": "Crossfire",
   "version": "1.5.5",
   "slug": "crossfire",
   "objective": "Runners must get from one side of the map to the other without getting killed by the Defenders.",
   "gamemode": [
      "ad",
      "blitz"
   ],
   "authors": [
      {
         "@uuid": "6e347123-c684-4d7e-996c-44d4924ff172"
      }
   ],
   "contributors": [
      {
         "@contribution": "Feedback",
         "#text": "BlockDrop74"
      },
      {
         "@contribution": "Feedback",
         "#text": "dinner1111"
      },
      {
         "@contribution": "Minor Terraforming",
         "#text": "Avaleer"
      },
      {
         "@contribution": "Help with Oberver Spawn",
         "#text": "yoyo_power"
      },
      {
         "@contribution": "XML Help",
         "#text": "Anthonny"
      }
   ],
   "rules": [
      "Fall Damage is Disabled.",
      "Do NOT Hide in the Houses!"
   ],
   "broadcasts": {
      "tip": {
         "@filter": "defenders-only",
         "@after": "5s",
         "@every": "1m",
         "#text": "Use the feather to go fast across the tracks"
      }
   },
   "destroyables": {
      "destroyable": {
         "@owner": "Defenders",
         "@name": "Monument",
         "@region": "monument",
         "@materials": "sponge",
         "@required": "true"
      }
   },
   "blitz": {
      "lives": "1",
      "broadcastLives": "true",
      "title": "Rush"
   },
   "include": {
      "@src": "blitz-global.xml"
   },
   "teams": [
      {
         "@color": "light purple",
         "@max": "25",
         "@max-overfill": "30",
         "@plural": "true",
         "#text": "Runners"
      },
      {
         "@color": "dark blue",
         "@max": "4",
         "@plural": "true",
         "#text": "Defenders"
      }
   ],
   "kits": [
      {
         "@name": "hunger",
         "saturation": "20",
         "foodlevel": "20"
      },
      {
         "@name": "magenta",
         "@parents": "hunger",
         "helmet": {
            "@color": "EBD2F5",
            "#text": "leather helmet"
         },
         "chestplate": {
            "@color": "EBD2F5",
            "#text": "leather chestplate"
         },
         "leggings": {
            "@color": "EBD2F5",
            "#text": "leather leggings"
         },
         "boots": {
            "@color": "FFFFFF",
            "#text": "leather boots"
         },
         "item": [
            {
               "@slot": "0",
               "@damage": "61",
               "#text": "wood sword"
            },
            {
               "@slot": "1",
               "#text": "boat"
            }
         ]
      },
      {
         "@name": "dark-blue",
         "@parents": "hunger",
         "helmet": {
            "@color": "293878",
            "#text": "leather helmet"
         },
         "chestplate": {
            "@color": "293878",
            "#text": "leather chestplate"
         },
         "leggings": {
            "@color": "293878",
            "#text": "leather leggings"
         },
         "boots": {
            "@color": "191919",
            "#text": "leather boots"
         },
         "potion": {
            "@duration": "600",
            "@amplifier": "2",
            "#text": "speed"
         },
         "item": [
            {
               "@slot": "1",
               "@unbreakable": "true",
               "@enchantment": "ARROW_KNOCKBACK:1;ARROW_DAMAGE:3;ARROW_INFINITE:1",
               "#text": "bow"
            },
            {
               "@slot": "8",
               "@lore": "`4Speedy Feather",
               "@attributes": "generic.movementSpeed:base:0.3",
               "#text": "feather"
            },
            {
               "@slot": "28",
               "@amount": "1",
               "#text": "arrow"
            }
         ]
      },
      {
         "@name": "heal",
         "potion": {
            "@duration": "3",
            "@amplifier": "4",
            "#text": "heal"
         }
      }
   ],
   "itemremove": [
      "string",
      "sponge",
      "arrow",
      "stick",
      "wood"
   ],
   "toolrepair": {
      "tool": "wood sword"
   },
   "killreward": {
      "filter": {
         "all": {
            "team": "Defenders",
            "kill-streak": {
               "@count": "3"
            }
         }
      },
      "kit": {
         "@force": "true",
         "item": {
            "@slot": "1",
            "@enchantment": "ARROW_KNOCKBACK:1;ARROW_DAMAGE:5;ARROW_INFINITE:1",
            "@name": "`AUpgraded Bow",
            "#text": "bow"
         }
      }
   },
