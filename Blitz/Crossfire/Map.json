         },
         {
            "@name": "Defender-Spawn",
            "@min": "756,87,405",
            "@max": "757,87,406"
         },
         {
            "@name": "Observer-Spawn",
            "@min": "756,95,375",
            "@max": "757,95,376"
         },
         {
            "@name": "Portal-A",
            "@min": "792,47,468",
            "@max": "793,48,1032"
         },
