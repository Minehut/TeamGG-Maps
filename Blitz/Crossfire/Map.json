            "@name": "Portal-B",
            "@min": "720,47,468",
            "@max": "721,48,1032"
         }
      ],
      "rectangle": {
         "@name": "Heal-1",
         "@min": "742,761",
         "@max": "770,762"
      },
      "union": [
         {
            "@name": "Spawn-A",
            "cuboid": [
               {
                  "@min": "761,86,405",
                  "@max": "762,87,409"
               },
               {
                  "@min": "759,86,408",
                  "@max": "762,87,409"
               }
            ]
